    "commitlint": "^17.3.0",
    "commitlint-config-gitmoji": "^2.2.8",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
